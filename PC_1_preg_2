#include <stdio.h>
int anio;
int bisiesto (int anio){
    return(anio%4==0 && (anio%100 != 0 || anio%400 ==0));
}

int main(){
    int inicio = 1900, fin;

    printf("Ingrese un año desde 1900: ");
    scanf("%d", &fin);
    printf("Años bisiestos y multiplos de 10 desde %d hasta %d: \n", inicio, fin);

    for(anio=inicio;anio<=fin;anio++){
        if(bisiesto(anio) && anio%10 ==0){
            printf("%d\n", anio);
        }
    }
    return 0;
}
